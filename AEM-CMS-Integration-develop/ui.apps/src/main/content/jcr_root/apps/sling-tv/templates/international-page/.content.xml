<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Template for creating International pages."
    jcr:primaryType="cq:Template"
    jcr:title="SlingTv International Page"
    allowedPaths="[/content/sling-tv(/.+)?]"
    ranking="{Long}100">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="sling-tv/components/page/international-page">
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <International-carousal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sling-tv/components/content/general/international-carousal"/>
            <rich-text-1
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sling-tv/components/content/general/rich-text"
                id="rte-int1"/>
            <rich-text-2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sling-tv/components/content/general/rich-text"
                id="rte-int2"/>
            <rich-text-3
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sling-tv/components/content/general/rich-text"
                id="rte-int3"/>
            <dynamic-grid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sling-tv/components/content/general/dynamic-grid"/>
            <grid-component
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sling-tv/components/content/general/grid-component"/>
            <device-ribbons
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sling-tv/components/content/general/device-ribbon"/>
            <rich-text-4
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sling-tv/components/content/general/rich-text"/>
            <cta-component1
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sling-tv/components/content/general/cta-component"
                id="cta-int1"/>
            <cta-component2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sling-tv/components/content/general/cta-component"
                id="cta-int2"/>
            <cta-component3
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sling-tv/components/content/general/cta-component"
                id="cta-int3"/>
        </par>
    </jcr:content>
</jcr:root>
